        "import numpy as np\n",
        "\n",
        "# ---------------- Similarity ----------------\n",
        "def cosine_sim(a, b):\n",
        "    return float(\n",
        "        np.dot(a, b) / (np.linalg.norm(a) * np.linalg.norm(b) + 1e-9)\n",
        "    )\n",
        "\n",
        "def cosine_distance(a, b):\n",
        "    return 1 - cosine_sim(a, b)\n",
        "\n",
        "\n",
        "# ---------------- Embed Text ----------------\n",
        "def embed_text(text, embed_model):\n",
        "    return np.array(embed_model.embed_query(text))\n",
        "\n",
        "\n",
        "# ---------------- Produce Answer with Given Chunks ----------------\n",
        "def generate_answer_with_chunks(question, chunks, llm):\n",
        "    context = \"\\n\\n\".join(c.page_content for c in chunks)\n",
        "    prompt = f\"Question: {question}\\n\\nContext:\\n{context}\\n\\nAnswer:\"\n",
        "    out = llm.invoke(prompt)\n",
        "    return out if isinstance(out, str) else str(out)\n",
        "\n",
        "\n",
        "# ---------------- LOO Causal Influence Score ----------------\n",
        "def loo_causal_influence_score(question, chunks, answer, generative_llm, embed_model, debug=False, debug_store=None):\n",
        "    \"\"\"\n",
        "    Leave-One-Out Causal Influence Score (LOO-CIS)\n",
        "    Based on:\n",
        "    - Jiang et al. 2023 (Hallucinations in RAG)\n",
        "    - Narayan et al. 2023 (Causal Effect of Retrieval)\n",
        "    - Pezeshkpour et al. 2023 (Causal Mediation)\n",
        "    \"\"\"\n",
        "\n",
        "    # ---- Answer with ALL chunks ----\n",
        "    #full_answer = generate_answer_with_chunks(question, chunks, ollama)\n",
        "    if debug and debug_store is not None:\n",
        "        debug_store[\"loo_causal_influence\"] = {\"per_chunk\": []}\n",
        "\n",
        "    full_emb = embed_text(answer, embed_model)\n",
        "    influences = []\n",
        "\n",
        "    for i in range(len(chunks)):\n",
        "        reduced_chunks = [c for j, c in enumerate(chunks) if j != i]\n",
        "        reduced_answer = generate_answer_with_chunks(question, reduced_chunks, generative_llm)\n",
        "        reduced_emb = embed_text(reduced_answer, embed_model)\n",
        "        dist = cosine_distance(full_emb, reduced_emb)\n",
        "        influences.append(dist)\n",
        "\n",
        "        if debug:\n",
        "            debug_store[\"loo_causal_influence\"][\"per_chunk\"].append({\n",
        "                \"removed_index\": i,\n",
        "                \"reduced_answer\": reduced_answer,\n",
        "                \"distance\": dist\n",
        "            })\n",
        "            print(f\"[loo] removed {i} | distance={dist:.4f}\")\n",
        "\n",
        "    mean_inf = float(np.mean(influences)) if influences else 0.0\n",
        "    if debug:\n",
        "        debug_store[\"loo_causal_influence\"][\"mean\"] = mean_inf\n",
        "        print(f\"[loo] mean causal influence: {mean_inf:.4f}\")\n",
        "    return mean_inf, influences\n"
      ]
