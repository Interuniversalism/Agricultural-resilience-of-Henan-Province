        "def unsupported_inference_ratio(question, answer, chunks, embed_model, llm, cosine_threshold=0.4, debug=False, debug_store=None):\n",
        "    \"\"\"\n",
        "    Unsupported Inference Ratio (UIR)\n",
        "    Academic-backed metric based on:\n",
        "    - Maynez et al. 2020 (ACL)\n",
        "    - Rashkin et al. 2021 (NAACL)\n",
        "    - Dziri et al. 2022 (EMNLP)\n",
        "    - Honovich et al. 2022 (ACL)\n",
        "    \"\"\"\n",
        "\n",
        "\n",
        "    \"\"\"\n",
        "    Measures how often the answer includes claims NOT supported by the retrieved chunks.\n",
        "    Returns a ratio: unsupported_paragraphs / total_paragraphs\n",
        "    \"\"\"\n",
        "\n",
        "    # Split answer into paragraphs\n",
        "    paragraphs = [p.strip() for p in answer.split(\"\\n\\n\") if p.strip()]\n",
        "\n",
        "    if debug and debug_store is not None:\n",
        "        debug_store[\"unsupported_inference_ratio\"] = {\"paragraphs\": []}\n",
        "\n",
        "    if not paragraphs:\n",
        "        if debug:\n",
        "            debug_store[\"unsupported_inference_ratio\"][\"final\"] = 0.0\n",
        "        return 0.0\n",
        "\n",
        "    # Extract chunk texts and pre-compute embeddings\n",
        "    chunk_texts = [c.page_content for c in chunks]\n",
        "    chunk_embs = embed_model.embed_documents(chunk_texts)\n",
        "\n",
        "    unsupported = 0\n",
        "\n",
        "    for pi, paragraph in enumerate(paragraphs):\n",
        "\n",
        "        # Compute paragraph embedding\n",
        "        p_emb = embed_model.embed_query(paragraph)\n",
        "\n",
        "        # Compute similarity to all chunks\n",
        "        sims = []\n",
        "        for ce in chunk_embs:\n",
        "            sim = float(np.dot(ce, p_emb) / (np.linalg.norm(ce) * np.linalg.norm(p_emb) + 1e-9))\n",
        "            sims.append(sim)\n",
        "\n",
        "        best_idx = int(np.argmax(sims))\n",
        "        best_chunk = chunk_texts[best_idx]\n",
        "        best_sim = sims[best_idx]\n",
        "\n",
        "        unsupported_flag = False\n",
        "        llm_raw = None\n",
        "        prompt = None\n",
        "\n",
        "        # Only evaluate unsupported inference if similarity is high\n",
        "        if best_sim >= cosine_threshold:\n",
        "            prompt = PROMPT_SUPPORTS_SENTENCE.format(\n",
        "                question=question,\n",
        "                chunk=best_chunk,\n",
        "                sentence=paragraph  # still calling it 'sentence' for prompt compatibility\n",
        "            )\n",
        "            llm_raw = llm_call(prompt, llm)\n",
        "\n",
        "            # If LLM says the paragraph contains unsupported inference\n",
        "            unsupported_flag = safe_yesno(llm_raw)\n",
        "\n",
        "        if unsupported_flag:\n",
        "            unsupported += 1\n",
        "\n",
        "        # Debug dump\n",
        "        if debug:\n",
        "            debug_store[\"unsupported_inference_ratio\"][\"paragraphs\"].append({\n",
        "                \"index\": pi,\n",
        "                \"paragraph\": paragraph,\n",
        "                \"best_chunk_index\": best_idx,\n",
        "                \"best_sim\": best_sim,\n",
        "                \"llm_prompt\": prompt,\n",
        "                \"llm_response\": llm_raw,\n",
        "                \"unsupported\": bool(unsupported_flag)\n",
        "            })\n",
        "\n",
        "            print(f\"[unsupported_inference_ratio] paragraph {pi} | \"\n",
        "                  f\"best_sim={best_sim:.4f} | unsupported={unsupported_flag}\")\n",
        "\n",
        "    final = unsupported / len(paragraphs)\n",
        "\n",
        "    if debug:\n",
        "        debug_store[\"unsupported_inference_ratio\"][\"final\"] = final\n",
        "        print(f\"[unsupported_inference_ratio] final ratio: {final:.4f}\")\n",
        "\n",
        "    return final\n"
      ]
    },
