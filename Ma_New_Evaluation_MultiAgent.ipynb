        "Context: '{chunk}'\n",
        "Answer sentence: '{sentence}'\n",
        "\n",
        "Does this context SUPPORT the sentence? Answer YES or NO.\n",
        "\"\"\"\n",
        "\n",
        "# ---------------- LLM CALL ----------------\n",
        "def llm_call(prompt, llm):\n",
        "    try:\n",
        "        out = llm.invoke(prompt)\n",
        "        if isinstance(out, str):\n",
        "            return out.strip()\n",
        "        return str(out).strip()\n",
        "    except:\n",
        "        return \"\"\n",
        "\n",
        "def safe_yesno(text):\n",
        "    t = (text or \"\").upper()\n",
        "    if \"YES\" in t: return True\n",
        "    if \"NO\" in t: return False\n",
        "    return False\n",
        "\n",
        "\n",
        "# ---------------- 1) Context Precision ----------------\n",
        "def context_precision(question, chunks, llm, debug=False, debug_store=None):\n",
        "    true_pos = 0\n",
        "    precisions = []\n",
        "    relevant_flags = []\n",
        "    if debug and debug_store is not None:\n",
        "        debug_store[\"context_precision\"] = {\"steps\": []}\n",
        "\n",
        "    for i, ch in enumerate(chunks):\n",
        "        prompt = PROMPT_IS_RELEVANT.format(question=question, chunk=ch.page_content)\n",
        "        resp = llm_call(prompt, llm)\n",
        "        is_rel = 1 if safe_yesno(resp) else 0\n",
        "\n",
        "        if is_rel:\n",
        "            true_pos += 1\n",
        "\n",
        "        prec = true_pos / (i + 1)\n",
        "        precisions.append(prec)\n",
        "        relevant_flags.append(is_rel)\n",
        "\n",
        "        if debug:\n",
        "            step = {\n",
        "                \"index\": i,\n",
        "                \"prompt\": prompt,\n",
        "                \"response_raw\": resp,\n",
        "                \"is_rel\": bool(is_rel),\n",
        "                \"true_pos\": true_pos,\n",
        "                \"precision\": prec\n",
        "            }\n",
        "            debug_store[\"context_precision\"][\"steps\"].append(step)\n",
        "            print(f\"[context_precision] chunk {i} | is_rel={is_rel} | precision={prec:.4f}\")\n",
        "\n",
        "    if sum(relevant_flags) == 0:\n",
        "        final = 0.0\n",
        "    else:\n",
        "        weighted = sum(p * r for p, r in zip(precisions, relevant_flags)) / sum(relevant_flags)\n",
        "        final = float(weighted)\n",
        "\n",
        "    if debug:\n",
