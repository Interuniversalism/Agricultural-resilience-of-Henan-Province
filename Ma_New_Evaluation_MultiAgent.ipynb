      "cell_type": "code",
      "source": [
        "# collabeval_judge.py\n",
        "# Requires: langchain_community.llms.Ollama (local Ollama), numpy, typing\n",
        "# Add this file to your project and import CollabEvalJudge where you used a single LLM.\n",
        "\n",
        "from langchain_community.llms import Ollama\n",
        "from typing import List, Dict, Any, Optional, Callable\n",
        "import numpy as np\n",
        "import time\n",
        "import math\n",
        "from typing import List, Dict\n",
        "from concurrent.futures import ThreadPoolExecutor, as_completed\n",
        "from langchain_community.llms import Ollama\n",
        "###############################################################################\n",
        "# Config / helper\n",
        "###############################################################################\n",
        "\n",
        "def safe_float(s: str, default=None):\n",
        "    try:\n",
        "        return float(s.strip())\n",
        "    except:\n",
        "        return default\n",
        "\n",
        "def majority_label_from_texts(texts: List[str], yes_tokens=(\"YES\",), no_tokens=(\"NO\",)):\n",
        "    t = [str(x).upper() if x is not None else \"\" for x in texts]\n",
        "    yes = sum(1 for x in t if any(tok in x for tok in yes_tokens))\n",
        "    no  = sum(1 for x in t if any(tok in x for tok in no_tokens))\n",
        "    if yes == no:\n",
        "        return None\n",
        "    return \"YES\" if yes > no else \"NO\"\n",
        "\n",
        "###############################################################################\n",
        "# Agent: wrapper around an Ollama model with persona prompts\n",
        "###############################################################################\n",
        "\n",
        "###############################################################################\n",
        "# Agent: wrapper around an Ollama model with persona prompts\n",
        "###############################################################################\n",
        "\n",
        "\n",
        "\n",
        "class Agent:\n",
        "    def __init__(self, model_name: str, persona: str = \"\", num_ctx: int = 8192):\n",
        "        self.model_name = model_name\n",
        "        self.persona = persona.strip()\n",
        "        self.llm = Ollama(model=self.model_name)\n",
        "\n",
        "    def run(self, prompt: str) -> str:\n",
        "        \"\"\"Generate output from the agent with persona applied.\"\"\"\n",
        "        if self.persona:\n",
        "            full_prompt = f\"{self.persona}\\n\\nUser: {prompt}\"\n",
        "        else:\n",
        "            full_prompt = prompt\n",
        "\n",
        "        try:\n",
        "            return self.llm.invoke(full_prompt)\n",
        "        except Exception as e:\n",
        "            return f\"[ERROR from {self.model_name}] {e}\"\n",
        "    def _compose(self, prompt: str) -> str:\n",
        "        if self.persona:\n",
        "            return f\"Role: {self.persona}\\n\\n{prompt}\"\n",
        "        return prompt\n",
        "\n",
        "    def ask(self, prompt: str) -> str:\n",
        "        \"\"\"Synchronous call to the underlying LLM.\"\"\"\n",
        "        full = self._compose(prompt)\n",
        "        try:\n",
        "            out = self.llm.invoke(full)\n",
        "            return str(out).strip()\n",
        "        except Exception as e:\n",
        "            return \"\"\n",
        "\n",
        "    def score_numeric(self, rubric_prompt: str) -> Optional[float]:\n",
        "        \"\"\"Ask the agent to produce a numeric 0-1 score. Returns float or None.\"\"\"\n",
        "        out = self.ask(rubric_prompt)\n",
        "        f = safe_float(out)\n",
        "        return f\n",
        "\n",
        "    def answer_text(self, prompt: str) -> str:\n",
        "        return self.ask(prompt)\n",
        "\n",
        "###############################################################################\n",
        "# CollabEvalCoordinator\n",
        "###############################################################################\n",
        "\n",
        "class CollabEvalJudge:\n",
        "    \"\"\"\n",
        "    Implements a CollabEval-style multi-agent judge.\n",
        "    Usage:\n",
        "      judge = CollabEvalJudge([\n",
        "          {\"model\":\"gemma2:2b\",\"persona\":\"Precise evaluator, conservative scoring\"},\n",
        "          {\"model\":\"llama3.2\",\"persona\":\"Focus on factuality and citations\"},\n",
        "          {\"model\":\"deepseek-r1:1.5b\",\"persona\":\"Focus on step-by-step reasoning\"},\n",
        "      ])\n",
        "      final = judge.evaluate(question, candidate_answer, context, rubric, mode=\"score\")\n",
        "    \"\"\"\n",
        "\n",
        "    def __init__(self, agents_config: List[Dict[str,str]],\n",
        "                 rounds: int = 1,\n",
        "                 aggregation: str = \"mean\",      # \"mean\", \"median\", \"weighted\", \"majority\"\n",
        "                 weights: Optional[List[float]] = None,\n",
        "                 unanimity_threshold: float = 0.6,\n",
        "                 debate_prompt_template: Optional[Callable] = None):\n",
        "        \"\"\"\n",
        "        agents_config: list of {\"model\":..., \"persona\":...}\n",
        "        rounds: number of discussion rounds (0 = skip debate)\n",
        "        aggregation: how to aggregate numeric scores\n",
        "        weights: optional list of same length as agents for weighted mean/majority\n",
        "        unanimity_threshold: fraction of agents that must agree to mark \"high_confidence\"\n",
        "        debate_prompt_template: optional function to build debate prompt\n",
        "        \"\"\"\n",
        "        self.agents = [Agent(cfg[\"model\"], persona=cfg.get(\"persona\",\"\")) for cfg in agents_config]\n",
        "        self.rounds = rounds\n",
        "        self.aggregation = aggregation\n",
        "        self.weights = np.array(weights) if weights is not None else None\n",
        "        self.unanimity_threshold = float(unanimity_threshold)\n",
        "        self.debate_prompt_template = debate_prompt_template or self.default_debate_prompt\n",
        "\n",
        "    # prompt builder helpers\n",
        "    @staticmethod\n",
        "    def build_rubric_prompt(question: str, candidate: str, context: str, rubric: str,\n",
        "                            instruction: str = \"Provide a score between 0 and 1, AND a short justification.\"):\n",
        "        \"\"\"\n",
        "        Returns a prompt that asks for numeric score + short explanation.\n",
        "        It is intentionally strict: first line numeric score, next lines explanation.\n",
        "        \"\"\"\n",
        "        p = f\"\"\"Question: {question}\n",
        "\n",
        "Context:\n",
        "{context}\n",
        "\n",
        "Candidate Answer:\n",
        "{candidate}\n",
        "\n",
        "Rubric:\n",
        "{rubric}\n",
        "\n",
        "{instruction}\n",
        "\n",
        "Output format:\n",
        "<number between 0 and 1>\n",
        "<short justification (one or two sentences)>\n",
        "\"\"\"\n",
        "        return p\n",
        "\n",
        "    @staticmethod\n",
        "    def default_debate_prompt(candidate: str, prev_scores: List[float], prev_explanations: List[str], agent_id: str):\n",
        "        \"\"\"\n",
        "        Default debate prompt: show others' scores+explanations, ask agent to optionally revise.\n",
        "        \"\"\"\n",
        "        others = \"\\n\\n\".join([f\"Agent{i} score={s:.3f}\\nExplanation: {e}\" for i,(s,e) in enumerate(zip(prev_scores, prev_explanations))])\n",
        "        p = f\"\"\"You are Agent {agent_id}.\n",
        "Here is the candidate answer:\n",
        "{candidate}\n",
        "\n",
        "Other agents' scores & explanations:\n",
        "{others}\n",
        "\n",
        "Based on the above, (1) state whether you want to CHANGE your numeric score (0-1).\n",
        "If you change it, output a new numeric score on the first line and a 1-2 sentence reason.\n",
        "If you keep it, output the original numeric score and a short confirmation sentence.\n",
        "\"\"\"\n",
        "        return p\n",
        "\n",
        "    # core evaluation flow\n",
        "    def evaluate(self, question: str, candidate: str, context: str, rubric: str,\n",
        "                 mode: str = \"score\") -> Dict[str,Any]:\n",
        "        \"\"\"\n",
        "        mode: \"score\" (numeric 0-1), \"yesno\" (binary judgment), \"text\" (best final text)\n",
        "        Returns dict with:\n",
        "          - 'per_agent': list of {\"id\",\"init_score\",\"init_explanation\",\"final_score\",\"final_explanation\"}\n",
        "          - 'aggregated_score': float (for score mode)\n",
        "          - 'aggregated_label': \"YES\"/\"NO\" or None (for yesno)\n",
        "          - 'consensus_rationale': str\n",
        "          - 'confidence': float  (based on agent agreement)\n",
        "        \"\"\"\n",
        "        n_agents = len(self.agents)\n",
        "        per_agent = [{\"id\":ag.model_name or f\"agent{i}\",\"init_score\":None,\"init_explanation\":\"\",\"final_score\":None,\"final_explanation\":\"\"} for i,ag in enumerate(self.agents)]\n",
        "\n",
        "        # Phase 1: initial scoring + justification\n",
        "        for i,ag in enumerate(self.agents):\n",
        "            rubric_prompt = self.build_rubric_prompt(question, candidate, context, rubric)\n",
        "            out = ag.answer_text(rubric_prompt)\n",
        "            # Parse: first token/line numeric, rest explanation\n",
        "            lines = [ln.strip() for ln in out.splitlines() if ln.strip()]\n",
        "            num = None\n",
        "            expl = \"\"\n",
        "            if lines:\n",
        "                f = safe_float(lines[0])\n",
        "                if f is not None:\n",
        "                    num = max(0.0, min(1.0, f))\n",
        "                    expl = \" \".join(lines[1:]) if len(lines)>1 else \"\"\n",
        "                else:\n",
        "                    # try find a number in text\n",
        "                    import re\n",
        "                    m = re.search(r\"([01](?:\\.\\d+)?)\", lines[0])\n",
        "                    if m:\n",
        "                        num = max(0.0, min(1.0, float(m.group(1))))\n",
        "                        expl = \" \".join(lines[1:]) if len(lines)>1 else \"\"\n",
        "                    else:\n",
        "                        # fallback: textual yes/no -> map YES=1, NO=0 or parse heuristics\n",
        "                        t = lines[0].upper()\n",
        "                        if \"YES\" in t: num = 1.0\n",
        "                        elif \"NO\" in t: num = 0.0\n",
        "                        else:\n",
        "                            num = 0.0\n",
        "                            expl = \" \".join(lines)\n",
        "            per_agent[i][\"init_score\"] = num\n",
        "            per_agent[i][\"init_explanation\"] = expl\n",
        "\n",
        "        # Phase 2: multi-round debate / revision\n",
        "        curr_scores = [pa[\"init_score\"] for pa in per_agent]\n",
        "        curr_expls = [pa[\"init_explanation\"] for pa in per_agent]\n",
        "\n",
        "        for rnd in range(self.rounds):\n",
        "            revised_scores = []\n",
        "            revised_expls = []\n",
        "            for i,ag in enumerate(self.agents):\n",
        "                prompt = self.debate_prompt_template(candidate, curr_scores, curr_expls, ag.model_name or str(i))\n",
        "                out = ag.answer_text(prompt)\n",
        "                lines = [ln.strip() for ln in out.splitlines() if ln.strip()]\n",
        "                num = None\n",
        "                expl = \"\"\n",
        "                if lines:\n",
        "                    f = safe_float(lines[0])\n",
        "                    if f is not None:\n",
        "                        num = max(0.0, min(1.0, f))\n",
        "                        expl = \" \".join(lines[1:]) if len(lines)>1 else \"\"\n",
        "                    else:\n",
        "                        # try extract number in text\n",
        "                        import re\n",
        "                        m = re.search(r\"([01](?:\\.\\d+)?)\", out)\n",
        "                        if m:\n",
        "                            num = max(0.0, min(1.0, float(m.group(1))))\n",
        "                            expl = \" \".join(lines[1:]) if len(lines)>1 else \"\"\n",
        "                        else:\n",
        "                            # no numeric change -> keep previous\n",
        "                            num = curr_scores[i]\n",
        "                            expl = out\n",
        "                revised_scores.append(num)\n",
        "                revised_expls.append(expl)\n",
        "\n",
        "            curr_scores = revised_scores\n",
        "            curr_expls = revised_expls\n",
        "\n",
        "        # store finals\n",
        "        for i in range(n_agents):\n",
        "            per_agent[i][\"final_score\"] = curr_scores[i]\n",
        "            per_agent[i][\"final_explanation\"] = curr_expls[i]\n",
        "\n",
        "        # Aggregation\n",
        "        final_score = None\n",
        "        if self.aggregation == \"mean\":\n",
        "            if self.weights is not None:\n",
        "                w = self.weights / np.sum(self.weights)\n",
        "                final_score = float(np.dot(curr_scores, w))\n",
        "            else:\n",
        "                final_score = float(np.mean(curr_scores))\n",
        "        elif self.aggregation == \"median\":\n",
        "            final_score = float(np.median(curr_scores))\n",
        "        elif self.aggregation == \"weighted\" and self.weights is not None:\n",
        "            w = self.weights / np.sum(self.weights)\n",
        "            final_score = float(np.dot(curr_scores, w))\n",
        "        else:\n",
        "            final_score = float(np.mean(curr_scores))\n",
        "\n",
        "        # For yesno mode: threshold final_score>=0.5 => YES\n",
        "        label = \"YES\" if final_score >= 0.5 else \"NO\" if mode==\"yesno\" else None\n",
        "\n",
        "        # consensus rationale: concatenate high-quality explanations from agents that are close to final_score\n",
        "        close_idx = [i for i,s in enumerate(curr_scores) if abs(s - final_score) <= 0.2]\n",
        "        if not close_idx:\n",
        "            close_idx = list(range(n_agents))\n",
        "        rationale_pieces = [per_agent[i][\"final_explanation\"] or per_agent[i][\"init_explanation\"] for i in close_idx]\n",
        "        consensus_rationale = \" // \".join([r for r in rationale_pieces if r])\n",
        "\n",
        "        # confidence: fraction of agents within +/-0.2 of final_score (or that produce same label)\n",
        "        agree_frac = sum(1 for s in curr_scores if abs(s - final_score) <= 0.2) / max(1, n_agents)\n",
        "        high_confidence = agree_frac >= self.unanimity_threshold\n",
        "\n",
        "        return {\n",
        "            \"per_agent\": per_agent,\n",
        "            \"aggregated_score\": final_score,\n",
        "            \"aggregated_label\": label,\n",
        "            \"consensus_rationale\": consensus_rationale,\n",
        "            \"agreement_fraction\": agree_frac,\n",
        "            \"high_confidence\": bool(high_confidence)\n",
        "        }\n",
        "\n",
        "###############################################################################\n",
        "# Adapter: make CollabEvalJudge behave like a single LLM for your functions\n",
        "###############################################################################\n",
        "\n",
        "class CollabEvalAdapter:\n",
        "    \"\"\"\n",
        "    A thin adapter to let CollabEvalJudge be passed as 'llm' into your existing functions.\n",
        "    It implements .invoke(prompt) and returns:\n",
        "      - for yes/no prompts -> \"YES\"/\"NO\"\n",
        "      - for numeric prompts (PROMPT_RELEVANCY_SCORE) -> \"0.72\" (string)\n",
        "      - for free-text generation -> consensus rationale or best agent text\n",
        "    You can tune detect_mode() for your prompts.\n",
        "    \"\"\"\n",
        "\n",
        "    def __init__(self, judge: CollabEvalJudge, detect_mode_fn: Optional[Callable[[str],str]] = None):\n",
        "        self.judge = judge\n",
        "        self.detect_mode_fn = detect_mode_fn or self.simple_detect_mode\n",
        "\n",
        "    def simple_detect_mode(self, prompt: str) -> str:\n",
        "        P = prompt.upper()\n",
        "        if \"RATE FROM 0 TO 1\" in P or \"RATE FROM 0 TO 1\" in P or \"RATE FROM 0 TO 1\" in P:\n",
        "            return \"score\"\n",
        "        if \"YES OR NO\" in P or \"ANSWER YES OR NO\" in P or \"DOES THIS\" in P:\n",
        "            return \"yesno\"\n",
        "        # fallback to text\n",
        "        return \"text\"\n",
        "\n",
        "    def invoke(self, prompt: str) -> str:\n",
        "        mode = self.detect_mode_fn(prompt)\n",
        "        # degrade: we need question, candidate, context, rubric from prompt.\n",
        "        # For portability we will pass the whole prompt as 'candidate' and use prompt as rubric/context.\n",
        "        # Better: call with structured args (see usage example below).\n",
        "        res = self.judge.evaluate(question=\"<<Q>>\", candidate=prompt, context=\"<<CTX>>\", rubric=\"Use the prompt to judge\", mode=\"score\" if mode==\"score\" else (\"yesno\" if mode==\"yesno\" else \"text\"))\n",
        "        if mode == \"score\":\n",
        "            return f\"{res['aggregated_score']:.4f}\"\n",
        "        if mode == \"yesno\":\n",
        "            return res[\"aggregated_label\"] or \"NO\"\n",
        "        # text mode: return consensus rationale or the longest agent explanation\n",
        "        if res[\"consensus_rationale\"]:\n",
        "            return res[\"consensus_rationale\"]\n",
        "        # fallback to best agent text\n",
        "        best = max(res[\"per_agent\"], key=lambda x: x[\"final_score\"] if x[\"final_score\"] is not None else -1)\n",
        "        return best.get(\"final_explanation\",\"\")\n",
        "        #######################################################################################################\n",
        "\n"
      ],
