        "import numpy as np\n",
        "from tqdm import trange\n",
        "import re\n",
        "\n",
        "def evaluate_alignment_consistency_ret_question(eval_llm, question, chunks, runs=5, debug=False, debug_store=None):\n",
        "    context_text = \"\\n\\n\".join([c.page_content for c in chunks])\n",
        "    scores = []\n",
        "    if debug and debug_store is not None:\n",
        "        debug_store[\"alignment_ret_question\"] = {\"scores\": []}\n",
        "\n",
        "    for _ in trange(runs, desc=\"alignment_ret_question\"):\n",
        "        prompt = PROMPT_ALIGNMENT_QUESTION.format(question=question, contexts=context_text)\n",
        "        out = eval_llm.invoke(prompt)\n",
        "        text = str(out).strip()\n",
        "        try:\n",
        "            score = float(text)\n",
        "        except:\n",
        "            match = re.search(r\"\\b([01]?\\.\\d+|[01])\\b\", text)\n",
        "            score = float(match.group(1)) if match else 0.0\n",
        "        scores.append(score)\n",
        "        if debug and debug_store is not None:\n",
        "            debug_store[\"alignment_ret_question\"][\"scores\"].append(score)\n",
        "\n",
        "    arr = np.array(scores)\n",
        "    res = {\"scores\": scores, \"mean\": float(arr.mean()), \"std\": float(arr.std()), \"min\": float(arr.min()), \"max\": float(arr.max())}\n",
        "    if debug and debug_store is not None:\n",
        "        debug_store[\"alignment_ret_question\"][\"summary\"] = res\n",
        "    return res\n",
        "\n",
        "    return {\n",
        "        \"scores\": scores.tolist(),\n",
        "        \"mean\": float(scores.mean()),\n",
        "        \"std\": float(scores.std()),\n",
        "        \"min\": float(scores.min()),\n",
        "        \"max\": float(scores.max())\n",
        "    }\n"
      ]
    },
