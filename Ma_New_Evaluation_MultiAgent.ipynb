        "\n",
        "\n",
        "\n",
        "class Agent:\n",
        "    def __init__(self, model_name: str, persona: str = \"\", num_ctx: int = 8192):\n",
        "        self.model_name = model_name\n",
        "        self.persona = persona.strip()\n",
        "        self.llm = Ollama(model=self.model_name)\n",
        "\n",
        "    def run(self, prompt: str) -> str:\n",
        "        \"\"\"Generate output from the agent with persona applied.\"\"\"\n",
        "        if self.persona:\n",
        "            full_prompt = f\"{self.persona}\\n\\nUser: {prompt}\"\n",
        "        else:\n",
        "            full_prompt = prompt\n",
        "\n",
        "        try:\n",
        "            return self.llm.invoke(full_prompt)\n",
        "        except Exception as e:\n",
        "            return f\"[ERROR from {self.model_name}] {e}\"\n",
        "    def _compose(self, prompt: str) -> str:\n",
        "        if self.persona:\n",
        "            return f\"Role: {self.persona}\\n\\n{prompt}\"\n",
        "        return prompt\n",
        "\n",
        "    def ask(self, prompt: str) -> str:\n",
        "        \"\"\"Synchronous call to the underlying LLM.\"\"\"\n",
        "        full = self._compose(prompt)\n",
        "        try:\n",
        "            out = self.llm.invoke(full)\n",
        "            return str(out).strip()\n",
        "        except Exception as e:\n",
        "            return \"\"\n",
        "\n",
        "    def score_numeric(self, rubric_prompt: str) -> Optional[float]:\n",
        "        \"\"\"Ask the agent to produce a numeric 0-1 score. Returns float or None.\"\"\"\n",
        "        out = self.ask(rubric_prompt)\n",
        "        f = safe_float(out)\n",
        "        return f\n",
        "\n",
        "    def answer_text(self, prompt: str) -> str:\n",
