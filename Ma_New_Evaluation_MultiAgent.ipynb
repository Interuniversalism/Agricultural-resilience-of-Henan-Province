        "            resp = llm_call(prompt, llm)\n",
        "            local_responses.append(resp)\n",
        "            local_scores.append(1.0 if safe_yesno(resp) else 0.0)\n",
        "\n",
        "        avg = sum(local_scores) / repeats\n",
        "        scores.append(avg)\n",
        "\n",
        "        if debug:\n",
        "            debug_store[\"context_relevance_dual\"][\"per_chunk\"].append({\n",
        "                \"index\": i,\n",
        "                \"responses\": local_responses,\n",
        "                \"avg_score\": avg\n",
        "            })\n",
        "            print(f\"[context_relevance_dual] chunk {i} | avg={avg:.4f}\")\n",
        "\n",
        "    final = float(sum(scores) / len(scores)) if scores else 0.0\n",
        "    if debug:\n",
        "        debug_store[\"context_relevance_dual\"][\"final\"] = final\n",
        "        print(f\"[context_relevance_dual] final: {final:.4f}\")\n",
        "    return final\n",
        "\n",
        "\n",
        "# ---------------- 3) Response Relevancy (LLM) ----------------\n",
        "def response_relevancy_llm(question, answer, llm, debug=False, debug_store=None):\n",
        "    prompt = PROMPT_RELEVANCY_SCORE.format(question=question, answer=answer)\n",
        "    resp = llm_call(prompt, llm)\n",
        "    num = extract_number(resp, default=None)\n",
        "    score = 0.0\n",
        "    if num is not None:\n",
        "        # clamp\n",
        "        score = max(0.0, min(1.0, float(num)))\n",
        "    if debug and debug_store is not None:\n",
        "        debug_store[\"response_relevancy_llm\"] = {\n",
        "            \"prompt\": prompt,\n",
        "            \"response_raw\": resp,\n",
        "            \"score\": score\n",
        "        }\n",
        "        print(f\"[response_relevancy_llm] score: {score:.4f} | raw: {resp}\")\n",
        "    return float(score)\n",
        "\n",
        "\n",
        "# ---------------- 4) Context Faithfulness ----------------\n",
        "def context_faithfulness(question, answer, chunks, embed_model, llm, cosine_threshold=0.4, debug=False, debug_store=None):\n",
        "    # Split by paragraphs instead of sentences\n",
        "    paragraphs = [p.strip() for p in answer.split(\"\\n\\n\") if p.strip()]\n",
        "\n",
        "    if debug and debug_store is not None:\n",
        "        debug_store[\"context_faithfulness\"] = {\"paragraphs\": []}\n",
        "\n",
        "    if not paragraphs:\n",
        "        if debug:\n",
        "            debug_store[\"context_faithfulness\"][\"final\"] = 0.0\n",
        "        return 0.0\n",
        "\n",
        "    chunk_texts = [c.page_content for c in chunks]\n",
        "    # cache chunk embeddings (fast!)\n",
        "    chunk_embs = embed_model.embed_documents(chunk_texts)\n",
        "\n",
        "    supported = 0\n",
        "\n",
        "    for pi, paragraph in enumerate(paragraphs):\n",
        "        p_emb = embed_model.embed_query(paragraph)\n",
        "\n",
        "        # similarity to each chunk\n",
        "        sims = []\n",
        "        for ce in chunk_embs:\n",
        "            sim = float(np.dot(ce, p_emb) / (np.linalg.norm(ce) * np.linalg.norm(p_emb) + 1e-9))\n",
        "            sims.append(sim)\n",
        "\n",
        "        best_idx = int(np.argmax(sims))\n",
        "        best_chunk = chunk_texts[best_idx]\n",
        "        best_sim = sims[best_idx]\n",
        "\n",
        "        supported_flag = False\n",
        "        llm_raw = None\n",
        "\n",
        "        if best_sim >= cosine_threshold:\n",
        "            prompt = PROMPT_SUPPORTS_SENTENCE.format(\n",
        "                question=question,\n",
        "                chunk=best_chunk,\n",
        "                sentence=paragraph  # keep name for prompt compatibility\n",
        "            )\n",
        "            llm_raw = llm_call(prompt, llm)\n",
        "            supported_flag = safe_yesno(llm_raw)\n",
        "\n",
        "        if supported_flag:\n",
        "            supported += 1\n",
        "\n",
        "        if debug:\n",
        "            debug_store[\"context_faithfulness\"][\"paragraphs\"].append({\n",
        "                \"index\": pi,\n",
        "                \"paragraph\": paragraph,\n",
        "                \"best_chunk_index\": best_idx,\n",
        "                \"best_sim\": best_sim,\n",
        "                \"llm_prompt\": None if best_sim < cosine_threshold else prompt,\n",
        "                \"llm_response\": llm_raw,\n",
        "                \"supported\": bool(supported_flag)\n",
        "            })\n",
        "            print(f\"[context_faithfulness] paragraph {pi} | best_sim={best_sim:.4f} | supported={supported_flag}\")\n",
        "\n",
        "    final = supported / len(paragraphs)\n",
        "\n",
        "    if debug:\n",
        "        debug_store[\"context_faithfulness\"][\"final\"] = final\n",
        "        print(f\"[context_faithfulness] final paragraph-level support ratio: {final:.4f}\")\n",
        "\n",
        "    return final\n",
        "\n",
        "\n",
        "\n",
        "# ============================================================\n",
        "#      Main Evaluation Function\n",
        "# ============================================================\n",
        "\n",
        "def evaluate_rag(question, answer, retrieved_docs, llm, embed_model):\n",
