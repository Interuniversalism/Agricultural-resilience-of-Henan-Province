        "# File: simplified_embed_chunk_store.py\n",
        "\n",
        "import os\n",
        "import json\n",
        "import torch\n",
        "from google.colab import drive\n",
        "from langchain_community.vectorstores import Chroma\n",
        "from langchain_core.embeddings import Embeddings\n",
        "from langchain_core.documents import Document\n",
        "from sklearn.metrics.pairwise import cosine_similarity\n",
        "\n",
        "def mount_drive():\n",
        "    drive.mount('/content/drive')\n",
        "\n",
        "def load_json_files_from_folder(folder_path):\n",
        "    all_chunks = []\n",
        "    for filename in os.listdir(folder_path):\n",
        "        if filename.endswith(\".json\"):\n",
        "            file_path = os.path.join(folder_path, filename)\n",
        "            with open(file_path, \"r\", encoding=\"utf-8\") as file:\n",
        "                try:\n",
        "                    chunks = json.load(file)\n",
        "                    all_chunks.extend([(chunk, filename) for chunk in chunks])\n",
        "                except json.JSONDecodeError:\n",
        "                    print(f\"❌ Error reading {filename}\")\n",
        "    return all_chunks\n",
        "\n",
        "class LlamaEmbeddings:\n",
        "    def __init__(self, base_url='http://localhost:11434', model=\"nomic-embed-text\"):\n",
        "        from langchain_community.embeddings import OllamaEmbeddings\n",
        "        self.model = OllamaEmbeddings(base_url=base_url, model=model)\n",
        "\n",
        "    def embed_query(self, text: str) -> list:\n",
        "        return self.model.embed_query(text)\n",
        "\n",
        "    def embed_documents(self, texts: list) -> list:\n",
        "        return self.model.embed_documents(texts)\n",
        "\n",
        "def add_documents_to_chromadb(vectorstore, chunks_with_meta):\n",
        "    try:\n",
        "        documents = [\n",
        "            Document(page_content=text, metadata={\"source_file\": meta})\n",
        "            for text, meta in chunks_with_meta\n",
        "        ]\n",
        "\n",
        "        vectorstore.add_documents(documents)\n",
        "        vectorstore.persist()\n",
        "        print(f\"✅ Added {len(documents)} documents to ChromaDB.\")\n",
        "        torch.cuda.empty_cache()\n",
        "    except Exception as e:\n",
        "        print(f\"❌ Error: {e}\")\n",
        "\n",
        "def retrieve_all_documents(vectorstore, k=500):\n",
        "    retriever = vectorstore.as_retriever()\n",
        "    #docs = retriever.get_relevant_documents(query=\"\", k=k)\n",
        "    docs = retriever.invoke(\"\")\n",
        "    return docs\n",
        "\n",
        "def main():\n",
        "    global vectorstore, all_chunks_with_meta, embedding_model\n",
        "    mount_drive()\n",
        "    folder_path = \"/content/drive/MyDrive/dimeniosns social jason\"\n",
        "    all_chunks_with_meta = load_json_files_from_folder(folder_path)\n",
        "\n",
        "    embedding_model = LlamaEmbeddings(base_url=\"http://localhost:11434\", model=\"nomic-embed-text\")\n",
        "    vectorstore = Chroma(persist_directory=\"./chroma_db\", embedding_function=embedding_model)\n",
        "\n",
        "    add_documents_to_chromadb(vectorstore, all_chunks_with_meta)\n",
        "\n",
        "    stored_chunks = retrieve_all_documents(vectorstore)\n",
        "    print(\"\\n✅ Retrieved Chunks from ChromaDB:\")\n",
        "    for idx, doc in enumerate(stored_chunks, 1):\n",
        "        print(f\"Chunk {idx} ({doc.metadata}):\\n{doc.page_content}\\n{'-'*80}\\n\")\n",
        "\n",
        "if __name__ == \"__main__\":\n",
        "    main()\n"
      ]
