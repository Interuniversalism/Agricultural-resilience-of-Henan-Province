        "def evaluate_all_metrics(\n",
        "        question,\n",
        "        answer,\n",
        "        retrieved_docs,\n",
        "        llm,               # evaluator / adapter LLM for yes/no and scoring\n",
        "        embed_model,\n",
        "        generative_llm,    # the model used in LOO to regenerate answers\n",
        "        eval_llm=None,\n",
        "        alignment_runs=5,\n",
        "        debug=True,\n",
        "        debug_output_path=\"evaluation_debug.json\"\n",
        "    ):\n",
        "    \"\"\"\n",
        "    Runs all metrics with debug traces:\n",
        "    - prints step-by-step\n",
        "    - returns (metrics_dict, debug_dict)\n",
        "    - writes debug_dict to debug_output_path (json)\n",
        "    \"\"\"\n",
        "    debug_store = {}\n",
        "    print(\"=== Starting full debug evaluation ===\")\n",
        "\n",
        "    # retrieval metrics (LLM-as-judge)\n",
        "    cp = context_precision(question, retrieved_docs, llm, debug=debug, debug_store=debug_store)\n",
        "    crd = context_relevance_dual(question, retrieved_docs, llm, debug=debug, debug_store=debug_store)\n",
        "\n",
        "    # answer quality\n",
        "    rrl = response_relevancy_llm(question, answer, llm, debug=debug, debug_store=debug_store)\n",
        "    cf = context_faithfulness(    question, answer, retrieved_docs, embed_model, llm,    debug=debug, debug_store=debug_store)\n",
        "\n",
        "    uir = unsupported_inference_ratio(    question, answer, retrieved_docs, embed_model, llm,    debug=debug, debug_store=debug_store)\n",
        "\n",
        "    # coverage & diversity\n",
        "    cov = context_coverage(answer, retrieved_docs, embed_model, debug=debug, debug_store=debug_store)\n",
        "    div = context_diversity(answer, retrieved_docs, embed_model, debug=debug, debug_store=debug_store)\n",
        "\n",
        "    # LOO causal influence\n",
        "    loo_mean, loo_list = loo_causal_influence_score(question, retrieved_docs, answer, generative_llm, embed_model, debug=debug, debug_store=debug_store)\n",
        "\n",
        "    # optional alignment stability\n",
        "    alignment_ret_answer = None\n",
        "    alignment_ret_question = None\n",
        "    if eval_llm is not None:\n",
        "        alignment_ret_answer = evaluate_alignment_consistency_ret_answer(eval_llm, question, answer, retrieved_docs, runs=alignment_runs, debug=debug, debug_store=debug_store)\n",
        "        alignment_ret_question = evaluate_alignment_consistency_ret_question(eval_llm, question, retrieved_docs, runs=alignment_runs, debug=debug, debug_store=debug_store)\n",
        "\n",
        "    metrics = {\n",
        "        # retrieval\n",
        "        \"context_precision\": cp,\n",
        "        \"context_relevance_dual\": crd,\n",
        "\n",
        "        # answer quality\n",
        "        \"response_relevancy_llm\": rrl,\n",
        "        \"context_faithfulness\": cf,\n",
        "        \"unsupported_inference_ratio\": uir,\n",
        "\n",
        "        # coverage & diversity\n",
        "        \"context_coverage\": cov,\n",
        "        \"context_diversity\": div,\n",
        "\n",
        "        # causal influence\n",
        "        \"loo_causal_influence_mean\": loo_mean,\n",
        "        \"loo_causal_influence_per_chunk\": loo_list,\n",
        "\n",
        "        # alignment (optional)\n",
        "        \"alignment_consistency_ret_answer\": alignment_ret_answer,\n",
        "        \"alignment_consistency_ret_question\": alignment_ret_question\n",
        "    }\n",
        "\n",
        "    # attach summary to debug_store\n",
        "    debug_store[\"metrics_summary\"] = metrics\n",
        "\n",
        "    # write debug to file\n",
        "    try:\n",
        "        with open(debug_output_path, \"w\", encoding=\"utf-8\") as f:\n",
        "            json.dump(debug_store, f, ensure_ascii=False, indent=2)\n",
        "        print(f\"Debug saved to {debug_output_path}\")\n",
        "    except Exception as e:\n",
        "        print(f\"Warning: failed to write debug file: {e}\")\n",
        "\n",
        "    print(\"=== Evaluation complete ===\")\n",
        "    return metrics, debug_store\n",
        "\n",
        "\n",
        "# ---------------- Prompts (keep your originals) ----------------\n",
        "PROMPT_IS_RELEVANT = \"\"\"\n",
        "Question: '{question}'\n",
        "Context chunk: '{chunk}'\n",
        "\n",
        "Does this chunk help answer the question? Answer YES or NO.\n",
