        "import numpy as np\n",
        "from sklearn.metrics.pairwise import cosine_similarity\n",
        "\n",
        "# ----------------------------------------\n",
        "# 🔹 Context Coverage\n",
        "# ----------------------------------------\n",
        "def context_coverage(answer, chunks, embed_model, threshold=0.7, debug=False, debug_store=None):\n",
        "    if debug and debug_store is not None:\n",
        "        debug_store[\"context_coverage\"] = {\"per_chunk\": []}\n",
        "\n",
        "    answer_vec = np.array(embed_model.embed_query(answer))\n",
        "    covered = 0\n",
        "\n",
        "    for i, chunk in enumerate(chunks):\n",
        "        chunk_vec = np.array(embed_model.embed_query(chunk.page_content))\n",
        "        sim = float(cosine_similarity([answer_vec], [chunk_vec])[0][0])\n",
        "        is_covered = sim >= threshold\n",
        "        if is_covered:\n",
        "            covered += 1\n",
        "        if debug:\n",
        "            debug_store[\"context_coverage\"][\"per_chunk\"].append({\n",
        "                \"index\": i,\n",
        "                \"sim\": sim,\n",
        "                \"covered\": bool(is_covered)\n",
        "            })\n",
        "            print(f\"[context_coverage] chunk {i} | sim={sim:.4f} | covered={is_covered}\")\n",
        "\n",
        "    final = covered / len(chunks) if chunks else 0.0\n",
        "    if debug:\n",
        "        debug_store[\"context_coverage\"][\"final\"] = final\n",
        "        print(f\"[context_coverage] final coverage: {final:.4f}\")\n",
        "    return final\n",
        "\n",
        "\n",
        "# ----------------------------------------\n",
        "# 🔹 Context Diversity\n",
        "# ----------------------------------------\n",
        "def context_diversity(answer, chunks, embed_model, reference_threshold=0.5, debug=False, debug_store=None):\n",
        "    if debug and debug_store is not None:\n",
        "        debug_store[\"context_diversity\"] = {\"relevant_indices\": [], \"pairwise\": []}\n",
        "\n",
        "    answer_vec = np.array(embed_model.embed_query(answer))\n",
        "    chunk_vecs = [np.array(embed_model.embed_query(c.page_content)) for c in chunks]\n",
        "\n",
        "    relevant = []\n",
        "    relevant_idxs = []\n",
        "    for i, vec in enumerate(chunk_vecs):\n",
        "        sim = float(cosine_similarity([answer_vec], [vec])[0][0])\n",
        "        if sim >= reference_threshold:\n",
        "            relevant.append(vec)\n",
        "            relevant_idxs.append(i)\n",
        "\n",
        "    if debug:\n",
        "        debug_store[\"context_diversity\"][\"relevant_indices\"] = relevant_idxs\n",
        "        print(f\"[context_diversity] relevant chunk indices: {relevant_idxs}\")\n",
        "\n",
        "    if len(relevant) <= 1:\n",
        "        if debug:\n",
        "            debug_store[\"context_diversity\"][\"final\"] = 0.0\n",
        "        return 0.0\n",
        "\n",
        "    sims = []\n",
        "    for i in range(len(relevant)):\n",
        "        for j in range(i + 1, len(relevant)):\n",
        "            s = float(cosine_similarity([relevant[i]], [relevant[j]])[0][0])\n",
        "            sims.append(s)\n",
        "            if debug:\n",
        "                debug_store[\"context_diversity\"][\"pairwise\"].append({\n",
        "                    \"i\": relevant_idxs[i],\n",
        "                    \"j\": relevant_idxs[j],\n",
        "                    \"sim\": s\n",
        "                })\n",
        "                print(f\"[context_diversity] pair ({relevant_idxs[i]},{relevant_idxs[j]}) sim={s:.4f}\")\n",
        "\n",
        "    avg_sim = np.mean(sims) if sims else 0.0\n",
        "    final = 1.0 - avg_sim\n",
        "    if debug:\n",
        "        debug_store[\"context_diversity\"][\"final\"] = final\n",
        "        print(f\"[context_diversity] final diversity: {final:.4f}\")\n",
        "    return final\n"
      ]
    },
