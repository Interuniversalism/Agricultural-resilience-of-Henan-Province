        "# ============================================================\n",
        "#   RAG Evaluation (Fully Consistent with Your Existing RAG)\n",
        "#   Uses SAME: ChromaDB, bge-m3 embeddings, retrieved docs\n",
        "#   No FAISS, No New Indexing, No New Retrieval\n",
        "# ============================================================\n",
        "import numpy as np\n",
        "from langchain_community.llms import Ollama\n",
        "from langchain_community.embeddings import OllamaEmbeddings\n",
        "\n",
        "# ---------------- PROMPTS ----------------\n",
        "PROMPT_IS_RELEVANT = \"\"\"\n",
        "Question: '{question}'\n",
        "Context chunk: '{chunk}'\n",
        "\n",
        "Does this chunk help answer the question? Answer YES or NO.\n",
        "\"\"\"\n",
        "\n",
        "PROMPT_RELEVANCY_SCORE = \"\"\"\n",
        "Question: '{question}'\n",
        "Answer: '{answer}'\n",
        "\n",
        "Rate from 0 to 1 how well the answer addresses the question.\n",
        "Only output a number between 0 and 1.\n",
        "\"\"\"\n",
        "\n",
        "PROMPT_SUPPORTS_SENTENCE = \"\"\"\n",
        "Question: '{question}'\n",
        "Context: '{chunk}'\n",
        "Answer sentence: '{sentence}'\n",
        "\n",
        "Does this context SUPPORT the sentence? Answer YES or NO.\n",
        "\"\"\"\n",
        "\n",
        "# ---------------- LLM CALL ----------------\n",
        "def llm_call(prompt, llm):\n",
        "    try:\n",
        "        out = llm.invoke(prompt)\n",
        "        if isinstance(out, str):\n",
        "            return out.strip()\n",
        "        return str(out).strip()\n",
        "    except:\n",
        "        return \"\"\n",
        "\n",
        "def safe_yesno(text):\n",
        "    t = (text or \"\").upper()\n",
        "    if \"YES\" in t: return True\n",
        "    if \"NO\" in t: return False\n",
        "    return False\n",
        "\n",
        "\n",
        "# ---------------- 1) Context Precision ----------------\n",
        "def context_precision(question, chunks, llm, debug=False, debug_store=None):\n",
        "    true_pos = 0\n",
        "    precisions = []\n",
        "    relevant_flags = []\n",
        "    if debug and debug_store is not None:\n",
        "        debug_store[\"context_precision\"] = {\"steps\": []}\n",
        "\n",
        "    for i, ch in enumerate(chunks):\n",
        "        prompt = PROMPT_IS_RELEVANT.format(question=question, chunk=ch.page_content)\n",
        "        resp = llm_call(prompt, llm)\n",
        "        is_rel = 1 if safe_yesno(resp) else 0\n",
        "\n",
        "        if is_rel:\n",
        "            true_pos += 1\n",
        "\n",
        "        prec = true_pos / (i + 1)\n",
        "        precisions.append(prec)\n",
        "        relevant_flags.append(is_rel)\n",
        "\n",
        "        if debug:\n",
        "            step = {\n",
        "                \"index\": i,\n",
        "                \"prompt\": prompt,\n",
        "                \"response_raw\": resp,\n",
        "                \"is_rel\": bool(is_rel),\n",
        "                \"true_pos\": true_pos,\n",
        "                \"precision\": prec\n",
        "            }\n",
        "            debug_store[\"context_precision\"][\"steps\"].append(step)\n",
        "            print(f\"[context_precision] chunk {i} | is_rel={is_rel} | precision={prec:.4f}\")\n",
        "\n",
        "    if sum(relevant_flags) == 0:\n",
        "        final = 0.0\n",
        "    else:\n",
        "        weighted = sum(p * r for p, r in zip(precisions, relevant_flags)) / sum(relevant_flags)\n",
        "        final = float(weighted)\n",
        "\n",
        "    if debug:\n",
        "        debug_store[\"context_precision\"][\"final\"] = final\n",
        "        print(f\"[context_precision] final weighted precision: {final:.4f}\")\n",
        "\n",
        "    return float(final)\n",
        "\n",
        "\n",
        "# ---------------- 2) Dual Context Relevance ----------------\n",
        "def context_relevance_dual(question, chunks, llm, debug=False, debug_store=None, repeats=4):\n",
        "    scores = []\n",
        "    if debug and debug_store is not None:\n",
        "        debug_store[\"context_relevance_dual\"] = {\"per_chunk\": []}\n",
        "\n",
        "    for i, ch in enumerate(chunks):\n",
        "        local_scores = []\n",
        "        local_responses = []\n",
        "        for _ in range(repeats):\n",
        "            prompt = PROMPT_IS_RELEVANT.format(question=question, chunk=ch.page_content)\n",
        "            resp = llm_call(prompt, llm)\n",
        "            local_responses.append(resp)\n",
        "            local_scores.append(1.0 if safe_yesno(resp) else 0.0)\n",
        "\n",
        "        avg = sum(local_scores) / repeats\n",
        "        scores.append(avg)\n",
        "\n",
        "        if debug:\n",
        "            debug_store[\"context_relevance_dual\"][\"per_chunk\"].append({\n",
        "                \"index\": i,\n",
        "                \"responses\": local_responses,\n",
        "                \"avg_score\": avg\n",
        "            })\n",
        "            print(f\"[context_relevance_dual] chunk {i} | avg={avg:.4f}\")\n",
        "\n",
        "    final = float(sum(scores) / len(scores)) if scores else 0.0\n",
        "    if debug:\n",
        "        debug_store[\"context_relevance_dual\"][\"final\"] = final\n",
        "        print(f\"[context_relevance_dual] final: {final:.4f}\")\n",
        "    return final\n",
        "\n",
        "\n",
        "# ---------------- 3) Response Relevancy (LLM) ----------------\n",
        "def response_relevancy_llm(question, answer, llm, debug=False, debug_store=None):\n",
        "    prompt = PROMPT_RELEVANCY_SCORE.format(question=question, answer=answer)\n",
        "    resp = llm_call(prompt, llm)\n",
        "    num = extract_number(resp, default=None)\n",
        "    score = 0.0\n",
        "    if num is not None:\n",
        "        # clamp\n",
        "        score = max(0.0, min(1.0, float(num)))\n",
        "    if debug and debug_store is not None:\n",
        "        debug_store[\"response_relevancy_llm\"] = {\n",
        "            \"prompt\": prompt,\n",
        "            \"response_raw\": resp,\n",
        "            \"score\": score\n",
        "        }\n",
        "        print(f\"[response_relevancy_llm] score: {score:.4f} | raw: {resp}\")\n",
        "    return float(score)\n",
        "\n",
        "\n",
        "# ---------------- 4) Context Faithfulness ----------------\n",
        "def context_faithfulness(question, answer, chunks, embed_model, llm, cosine_threshold=0.4, debug=False, debug_store=None):\n",
        "    # Split by paragraphs instead of sentences\n",
        "    paragraphs = [p.strip() for p in answer.split(\"\\n\\n\") if p.strip()]\n",
        "\n",
        "    if debug and debug_store is not None:\n",
        "        debug_store[\"context_faithfulness\"] = {\"paragraphs\": []}\n",
        "\n",
        "    if not paragraphs:\n",
        "        if debug:\n",
        "            debug_store[\"context_faithfulness\"][\"final\"] = 0.0\n",
        "        return 0.0\n",
        "\n",
        "    chunk_texts = [c.page_content for c in chunks]\n",
        "    # cache chunk embeddings (fast!)\n",
        "    chunk_embs = embed_model.embed_documents(chunk_texts)\n",
        "\n",
        "    supported = 0\n",
        "\n",
        "    for pi, paragraph in enumerate(paragraphs):\n",
        "        p_emb = embed_model.embed_query(paragraph)\n",
        "\n",
        "        # similarity to each chunk\n",
        "        sims = []\n",
        "        for ce in chunk_embs:\n",
        "            sim = float(np.dot(ce, p_emb) / (np.linalg.norm(ce) * np.linalg.norm(p_emb) + 1e-9))\n",
        "            sims.append(sim)\n",
        "\n",
        "        best_idx = int(np.argmax(sims))\n",
        "        best_chunk = chunk_texts[best_idx]\n",
        "        best_sim = sims[best_idx]\n",
        "\n",
        "        supported_flag = False\n",
        "        llm_raw = None\n",
        "\n",
        "        if best_sim >= cosine_threshold:\n",
        "            prompt = PROMPT_SUPPORTS_SENTENCE.format(\n",
        "                question=question,\n",
        "                chunk=best_chunk,\n",
        "                sentence=paragraph  # keep name for prompt compatibility\n",
        "            )\n",
        "            llm_raw = llm_call(prompt, llm)\n",
        "            supported_flag = safe_yesno(llm_raw)\n",
        "\n",
        "        if supported_flag:\n",
        "            supported += 1\n",
        "\n",
        "        if debug:\n",
        "            debug_store[\"context_faithfulness\"][\"paragraphs\"].append({\n",
        "                \"index\": pi,\n",
        "                \"paragraph\": paragraph,\n",
        "                \"best_chunk_index\": best_idx,\n",
        "                \"best_sim\": best_sim,\n",
        "                \"llm_prompt\": None if best_sim < cosine_threshold else prompt,\n",
        "                \"llm_response\": llm_raw,\n",
        "                \"supported\": bool(supported_flag)\n",
        "            })\n",
        "            print(f\"[context_faithfulness] paragraph {pi} | best_sim={best_sim:.4f} | supported={supported_flag}\")\n",
        "\n",
        "    final = supported / len(paragraphs)\n",
        "\n",
        "    if debug:\n",
        "        debug_store[\"context_faithfulness\"][\"final\"] = final\n",
        "        print(f\"[context_faithfulness] final paragraph-level support ratio: {final:.4f}\")\n",
        "\n",
        "    return final\n",
        "\n",
        "\n",
        "\n",
        "# ============================================================\n",
        "#      Main Evaluation Function\n",
        "# ============================================================\n",
        "\n",
        "def evaluate_rag(question, answer, retrieved_docs, llm, embed_model):\n",
        "    return {\n",
        "        \"context_precision\": context_precision(question, retrieved_docs, llm),\n",
        "        \"context_relevance_dual\": context_relevance_dual(question, retrieved_docs, llm),\n",
        "        \"response_relevancy_llm\": response_relevancy_llm(question, answer, llm),\n",
        "        \"context_faithfulness\": context_faithfulness(\n",
        "            question, answer, retrieved_docs, embed_model, llm\n",
        "        ),\n",
        "    }\n"
      ]
    },
