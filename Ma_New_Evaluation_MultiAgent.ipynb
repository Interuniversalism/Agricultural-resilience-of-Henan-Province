        "from concurrent.futures import ThreadPoolExecutor, as_completed\n",
        "\n",
        "def evaluate_agents_parallel(judge, question, candidate, context, rubric, mode=\"score\"):\n",
        "        results = {}\n",
        "        with ThreadPoolExecutor() as executor:\n",
        "            futures = {executor.submit(judge.evaluate, question, candidate, context, rubric, mode): i for i in range(len(judge.agents))}\n",
        "            for future in as_completed(futures):\n",
        "                idx = futures[future]\n",
        "                try:\n",
        "                    results[idx] = future.result()\n",
        "                except Exception as e:\n",
        "                    results[idx] = {\"error\": str(e)}\n",
        "        return results"
