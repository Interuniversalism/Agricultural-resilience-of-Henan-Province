        "def safe_yesno(text, numeric_threshold=0.5):\n",
        "    \"\"\"\n",
        "    Robust yes/no detection:\n",
        "    - If starts with yes/no (case-insensitive) -> use that\n",
        "    - Else try to extract a number in [0,1] and threshold it\n",
        "    - Else fallback False\n",
        "    \"\"\"\n",
        "    if text is None:\n",
        "        return False\n",
        "    t = str(text).strip()\n",
        "    # explicit yes/no at beginning\n",
        "    m = re.match(r\"^\\s*(yes|y|no|n)\\b\", t, flags=re.I)\n",
        "    if m:\n",
        "        return m.group(1).lower().startswith('y')\n",
        "    # extract a float 0..1\n",
        "    mnum = re.search(r\"([-+]?\\d*\\.?\\d+)\", t)\n",
        "    if mnum:\n",
        "        try:\n",
        "            val = float(mnum.group(1))\n",
        "            # if val looks like percent (e.g., 70) convert if >1\n",
        "            if val > 1:\n",
        "                # assume percent\n",
        "                val = val / 100.0\n",
        "            return val >= numeric_threshold\n",
        "        except:\n",
        "            pass\n",
        "    return False\n",
        "\n",
        "def extract_number(text, default=None):\n",
        "    if text is None:\n",
        "        return default\n",
        "    m = re.search(r\"([-+]?\\d*\\.?\\d+)\", str(text))\n",
        "    if m:\n",
        "        try:\n",
        "            return float(m.group(1))\n",
        "        except:\n",
        "            return default\n",
        "    return default\n",
        "\n",
        "def cosine_sim(a, b):\n",
        "    a = np.array(a, dtype=float)\n",
        "    b = np.array(b, dtype=float)\n",
        "    denom = (np.linalg.norm(a) * np.linalg.norm(b) + 1e-9)\n",
        "    return float(np.dot(a, b) / denom)\n",
        "\n",
        "def cosine_distance(a, b):\n",
        "    return 1.0 - cosine_sim(a, b)\n",
        "\n",
        "def embed_text(text, embed_model):\n",
        "    return np.array(embed_model.embed_query(text))\n",
        "\n",
        "\n",
        "# ---------------- LLM wrapper ----------------\n",
        "def llm_call(prompt, llm):\n",
