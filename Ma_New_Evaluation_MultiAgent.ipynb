        "import torch\n",
        "import numpy as np\n",
        "import gc\n",
        "from sklearn.metrics.pairwise import cosine_similarity\n",
        "\n",
        "# ============================================================\n",
        "# 🔹 PROMPT: DPSIR for Agricultural Resilience (Henan)\n",
        "# ============================================================\n",
        "#prompt_1085 = \"\"\"\n",
        "#Task:\n",
        "#Identify from any given data or document the ecological and social variables that correspond to each of the Drivers–Pressures–States–Impacts–Responses categories, specifically in relation to resilience of agriculture in Henan. Explain how categories relate between categories (e.g., how a Driver leads to a Pressure).\n",
        "#\"\"\"\n",
        "\n",
        "# ============================================================\n",
        "# 🔹 COSINE SIMILARITY FUNCTION\n",
        "# ============================================================\n",
        "def cosine_sim(a, b):\n",
        "    return float(np.dot(a, b) / (np.linalg.norm(a) * np.linalg.norm(b) + 1e-9))\n",
        "\n",
        "# ============================================================\n",
        "# 🔹 RAG + COSINE SIMILARITY COMBINED\n",
        "# ============================================================\n",
        "def simple_rag_query_with_similarity(question, vectorstore, llm, embed_model, k=4):\n",
        "    global  answer_eco, full_context_eco, answer_doc_similarity_eco , answer_question_similarity_eco , docs_eco\n",
        "    try:\n",
        "        # 1️⃣ Retrieve top-k documents\n",
        "        retriever = vectorstore.as_retriever(search_kwargs={\"k\": k})\n",
        "        docs_eco = retriever.invoke(question)\n",
        "\n",
        "        # 2️⃣ Embed the question\n",
        "        q_emb = np.array(embed_model.embed_query(question))\n",
        "        cosine_scores = []\n",
        "        context_parts = []\n",
        "\n",
        "        for i, doc_eco in enumerate(docs_eco):\n",
        "            chunk_text = doc_eco.page_content\n",
        "            c_emb = np.array(embed_model.embed_query(chunk_text))\n",
        "            sim = cosine_sim(q_emb, c_emb)\n",
        "            cosine_scores.append(sim)\n",
        "            context_parts.append(f\"[Document {i+1} | Cosine={sim:.4f}]: {chunk_text}\")\n",
        "\n",
        "        full_context_eco = \"\\n\\n\".join(context_parts)\n",
        "\n",
        "        # 3️⃣ Construct the final LLM prompt\n",
        "        prompt = f\"\"\"You are an expert analyst. Based on the following documents, answer the question thoroughly.the five DPSIR(Driver, Pressure, State, Impact, Response) framework definition in Agricultural resilience context as below:\n",
        "Driver: Broad socio-economic or environmental forces (e.g., policy reforms, climate variability, demographic trends) that shape agricultural systems over time.\n",
        "Pressure: Direct human-induced or natural stressors on agroecosystems (e.g., land degradation, overuse of agrochemicals, irrigation intensity).\n",
        "State: Current condition of the agroecosystem and socio-economic attributes (e.g., soil fertility, water availability, biodiversity, farmer income, land tenure).\n",
        "Impact: Consequences of changes in the state (e.g., reduced crop yields, increased poverty, food insecurity, ecological degradation).\n",
        "Response: Actions or interventions (e.g., adaptive farming practices, subsidies, training programs, policy changes) to mitigate pressures or enhance resilience.\n",
        "\n",
        "        QUESTION: {question}\n",
        "        DOCUMENTS:\n",
        "        {full_context_eco}\n",
        "        Please provide a comprehensive answer based only on the provided documents:\"\"\"\n",
        "\n",
        "        # 4️⃣ Generate the answer\n",
        "        answer_eco = llm.invoke(prompt)\n",
        "\n",
        "        # 5️⃣ Compute answer similarities\n",
        "        answer_vec = np.array(embed_model.embed_query(answer_eco))\n",
        "        prompt_vec = np.array(embed_model.embed_query(question))\n",
        "        context_vec = np.array(embed_model.embed_query(full_context_eco))\n",
        "\n",
        "        answer_question_similarity_eco = cosine_sim(answer_vec, prompt_vec)\n",
        "        answer_doc_similarity_eco = cosine_sim(answer_vec, context_vec)\n",
        "\n",
        "        # 6️⃣ Display results\n",
        "        print(\"\\n================= 🔍 RAG ANSWER =================\")\n",
        "        print(answer_eco)\n",
        "\n",
        "        print(\"\\n================= 📚 SOURCE DOCUMENTS =================\")\n",
        "        for i, doc_eco in enumerate(docs_eco):\n",
        "            print(f\"\\n📄 Document {i+1}: Cosine Similarity = {cosine_scores[i]:.6f}\")\n",
        "            print(f\"Metadata: {doc_eco.metadata}\")\n",
        "            print(f\"Content: {doc_eco.page_content[:8000]}{'...' if len(doc_eco.page_content) > 8000 else ''}\")\n",
        "            print(\"-\" * 80)\n",
        "\n",
        "        print(\"\\n================= 📊 SIMILARITY METRICS =================\")\n",
        "        print(f\"Answer–Question similarity: {answer_question_similarity_eco:.4f}\")\n",
        "        print(f\"Answer–Context similarity: {answer_doc_similarity_eco:.4f}\")\n",
        "\n",
        "        return {\n",
        "            \"answer\": answer_eco,\n",
        "            \"docs\": docs_eco,\n",
        "            \"cosine_scores\": cosine_scores,\n",
        "            \"answer_question_similarity\": answer_question_similarity_eco,\n",
        "            \"answer_doc_similarity\": answer_doc_similarity_eco,\n",
        "            \"context\": full_context_eco\n",
        "        }\n",
        "\n",
        "    except Exception as e:\n",
        "        return {\"error\": str(e)}\n",
        "\n",
        "# ============================================================\n",
        "# 🔹 RUN\n",
        "# ============================================================\n",
        "print(\"🔍 Processing your RAG query with cosine similarity...\\n\")\n",
        "\n",
        "result = simple_rag_query_with_similarity(\n",
        "    question=prompt_1085,\n",
        "    vectorstore=eco_vectorstore,  # Replace with your vectorstore\n",
        "    llm=ollama,  # Replace with your LLM\n",
        "    embed_model=ollama_embeddings,  # Replace with your embedding model\n",
        "    k=4\n",
        ")\n",
        "\n",
        "if \"error\" in result:\n",
        "    print(f\"❌ Error: {result['error']}\")\n"
      ]
    },
