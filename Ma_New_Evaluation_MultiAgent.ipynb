        "\n",
        "def compute_cosine(a, b):\n",
        "    return float(cosine_similarity([a], [b])[0][0])\n",
        "\n",
        "def compute_answer_similarities(answer, prompt, context, embedding_model):\n",
        "    global answer_question_similarity, answer_doc_similarity\n",
        "    # 🔹 ساخت embedding‌ها\n",
        "    answer_vec = embedding_model.embed_query(answer)\n",
        "    prompt_vec = embedding_model.embed_query(prompt)\n",
        "    context_vec = embedding_model.embed_query(context)\n",
        "\n",
        "    # 🔹 شباهت پاسخ با سؤال\n",
        "    answer_question_similarity = compute_cosine(answer_vec, prompt_vec)\n",
        "\n",
        "    # 🔹 شباهت پاسخ با کل context (یعنی همه‌ی اسناد با هم)\n",
        "    answer_doc_similarity = compute_cosine(answer_vec, context_vec)\n",
        "\n",
        "    return {\n",
        "        \"answer_doc_similarity\": answer_doc_similarity,\n",
        "        \"answer_question_similarity\": answer_question_similarity\n",
        "    }\n",
        "\n",
        "# 🧠 استفاده:\n",
        "scores = compute_answer_similarities(answer, prompt_1085, full_context, ollama_embeddings)\n",
        "\n",
        "print(f\"Answer–Question similarity: {scores['answer_question_similarity']:.4f}\")\n",
        "print(f\"Answer–Context similarity: {scores['answer_doc_similarity']:.4f}\")\n"
